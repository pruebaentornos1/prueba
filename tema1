public class B2_Ej14_Rango {

    /**
     * Realizar un programa que lea un numero entero por teclado y nos muestre si es
"mayor de 10 y menor de 20" o "mayor de 15 y menor de 25".
     */
    public static void main(String[] args) {
        Scanner teclado=new Scanner(System.in);
        String cad;
        int num;
        
        System.out.println("Dime un numero");
        cad=teclado.nextLine();
        num=Integer.parseInt(cad);
        
        if (num>10 && num<20)
        {
            System.out.println("Mayor de 10 y menor de 20");
        }
        
        
        if (num>15 && num<25)
        {
            System.out.println("Mayor de 15 y menor de 25");
        }
        
    }
}

