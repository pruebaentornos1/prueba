Pseudocodigo Total10

import java.util.Scanner;

public class Pseudocodigo_Total10 {

    
    public static void main(String[] args) {
        Scanner tec=new Scanner(System.in);
        String cad;
        
        int contador;
        boolean hay10;
        double nota;
        
        hay10=false;
        
        for (contador=1;contador<11;contador++)
        {
            System.out.println("Dime la nota");
            cad=tec.nextLine();
            nota=Double.parseDouble(cad);
            
            if (nota==10)
            {
                hay10=true;
            }
        }
        if (hay10==true)
        {
            System.out.println("Si, al menos hay un 10 en las notas");
        }
        else
        {
            System.out.println("No hay ningun 10 en las notas");
        }
    }
}
