B3-ej3

import java.util.Scanner;

public class B3_3 {

   
    public static void main(String[] args) {
        Scanner tec=new Scanner(System.in);
        String cad;
        int n1,n2,n3,n4,n5,media;
        
        System.out.println("Dime el primer numero");
        cad=tec.nextLine();
        n1=Integer.parseInt(cad);
        
        System.out.println("Dime el segundo numero");
        cad=tec.nextLine();
        n2=Integer.parseInt(cad);
        
        System.out.println("Dime el tercer numero");
        cad=tec.nextLine();
        n3=Integer.parseInt(cad);
        
        System.out.println("Dime el cuarto numero");
        cad=tec.nextLine();
        n4=Integer.parseInt(cad);
        
        System.out.println("Dime el quinto numero");
        cad=tec.nextLine();
        n5=Integer.parseInt(cad);
        
        media= (n1+n2+n3+n4+n5)/5;
        
        System.out.println("La media es "+media);
        
    }
}

