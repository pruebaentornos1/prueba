public class Pseudocodigo_Comprendidos {

    
    public static void main(String[] args) {
        
        /*int contador;
         contador=100;
         while (contador>=90)
         {
             System.out.println(contador);
             contador=contador-1;
         }
         */
        
        for (int contador=100;contador>=90;contador--)
        {
            System.out.println(contador);
        }
    }
}

