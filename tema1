B3-ej8

import java.util.Scanner;
        
public class B3_Ej8 {

    /**
     * interes 2% ; capital 6000€ ; n = años ; interes 1 año 6000*0.02 =6000+120 =6120
     *                                         interes 2 año 6120*0.02 =6120+122.4=6242.4
     */
    public static void main(String[] args) {
        Scanner tec=new Scanner(System.in);
        double intereses;
        int n;
        final double interes=0.02;
        double capital=6000;
                
                System.out.println("Dime los años que quieres mantener el dinero");
                n=tec.nextInt();
                
                
                int contador;
                
                for (contador=1;contador<=n;contador++)
                {
                    intereses=interes*capital;                       /* Se resume capital+=capital*interes
                    capital=capital+intereses; //capital+=intereses; */
                }
                System.out.println("El dinero que se va a sacar es "+capital);
    }
}

