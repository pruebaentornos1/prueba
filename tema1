Pseudocodigo 9

import java.util.Scanner;

public class Pseudocodigo_9_Notas {

   
    public static void main(String[] args) {
        Scanner teclado=new Scanner(System.in);
        String cad;
        char letra;
        
        System.out.println("Dime la nota en el sistema americano");
        cad=teclado.nextLine(); // cad=cad.toUpperCase();
        letra=cad.charAt(0); //Leer una letra. --- letra=cad.chartAt(0);
        
        switch (letra)
        {
            case 'A':case 'a': System.out.println("SB"); break;
            case 'B':case 'b': System.out.println("NT"); break;
            case 'C':case 'c': System.out.println("B"); break;
            case 'D':case 'd': System.out.println("SF"); break;
            case 'E':case 'e': System.out.println("INSF"); break;
            case 'F':case 'f': System.out.println("No presentado"); break;
            default : System.out.println("La nota es incorrecta");
        }
    }
}

