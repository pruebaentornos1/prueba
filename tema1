Pseudocodigo 15alumnos1

import java.util.Scanner;

public class Pseudocodigo_15alumnos1 {

    
    public static void main(String[] args) {
        Scanner tec=new Scanner(System.in);
        String cad;
        int numA,numS,numAlumnos;
        double suma,nota,media;
        
        numAlumnos=0;
        numA=0;
        numS=0;
        suma=0;
        
            System.out.println("Dime la nota");
            cad=tec.nextLine();
            nota=Double.parseDouble(cad);
            
            while (nota!=-1)
            {
            if (nota<5)
            {
                numS=numS+1;
            }
            else
            {
                numA=numA+1;
            }
            suma=suma+nota;
            numAlumnos=numAlumnos+1;
            
            System.out.println("Dime la nota");
            cad=tec.nextLine();
            nota=Double.parseDouble(cad);
        }
        System.out.println("Total de alumnos: "+numAlumnos);
        System.out.println(numA+" aprobados");
        System.out.println(numS+" suspensos");
        
        media=suma/15;
        System.out.println(media+" nota media");
    }
}

