Pseudocodigo 6

/*
 * To change this template, choose Tools | Templates
 * and open the template in the editor.
 */
import java.util.Scanner;
/**
 *
 * @author xps
 */
public class Pseudocodigo_6 {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        Scanner teclado=new Scanner(System.in);
        String cad;
        String nombre;
        int horas,horasextra;
        double tarifa,sB,sN,tasas;
        
        System.out.println("Dime el nombre");
        nombre=teclado.nextLine();
        
        System.out.println("Dime las horas trabajadas");
        cad=teclado.nextLine();
        horas=Integer.parseInt(cad);
        
        System.out.println("Dime la tarifa");
        cad=teclado.nextLine();
        tarifa=Double.parseDouble(cad);
        
        //Obtener Salario Bruto
        if (horas>35)
        {
            sB=35*tarifa+(horas-35)*tarifa*1.5;
            /*horasextra=horas-35;
             *sB=35*tarifa+horasextra*tarifa*1.5;
             */
        }
        else
        {
            sB=horas*tarifa;
        }
        
        //Obtener Tasas
        
        if (sB<=100)
        {
            tasas=0;
        }
        else
            if (sB<=200)
            {
                tasas=(sB-100)*0.1;
            }
        else
            {
                tasas=100*0.1+(sB-200)*0.25;
            }
        
        //Obtener Salario Neto
        
        sN=sB-tasas;
        
        System.out.println("El trabajador "+nombre+" con salario bruto "+sB+"€ paga de impuestos "+tasas+" y cobra "+sN);
    }
}

