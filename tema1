import java.util.Scanner;
/**
 *
 * @author xps
 */
public class Pseudocodigo_4_Salario2 {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        Scanner teclado=new Scanner(System.in);
        String cad;
        double preciohora,salario;
        int horas,horasextra;
        
        System.out.println("Dime el nombre ");
        String nombre=teclado.nextLine();
        
        System.out.println("Dime las horas trabajadas");
        cad=teclado.nextLine();
        horas=Integer.parseInt(cad);
        
        System.out.println("Dime el precio de la hora");
        cad=teclado.nextLine();
        preciohora=Double.parseDouble(cad);
        
        if (horas>40)
        {
            horasextra=horas-40;
            salario=40*preciohora+horasextra*preciohora*1.5;
        }
        else
        {
            salario=40*preciohora;
        }
        
        System.out.println(nombre+" cobra "+salario+" euros");
        
        /*horasextra=0;
        
        if (horas>40)
        {
            horasextra=horas-40;
        }
        salario=40*preciohora+horasextra*preciohora*1.5;
        
        System.out.println(nombre+" cobra "+salario+" euros");
        */
      
        
    }
}

