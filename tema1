import java.util.Scanner;
/**
 *
 * @author xps
 */
public class Pseudocodigo_3_Salario {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        Scanner teclado=new Scanner(System.in);
        String cad;;
        double preciohora,salariobruto,salarioneto;
        int horas;
        
        System.out.println("Dime el nombre del empleado");
        String nombre=teclado.nextLine();
        
        System.out.println("Dime las horas trabajadas");
        cad=teclado.nextLine();
        horas=Integer.parseInt(cad);
        
        System.out.println("Dime el precio de la hora");
        cad=teclado.nextLine();
        preciohora=Double.parseDouble(cad);
        
        salariobruto=horas*preciohora;
        salarioneto=salariobruto*0.9;
        
        System.out.println("El trabajador "+nombre+" cobrara de salario neto "+salarioneto+" euros");
        
    }
}

