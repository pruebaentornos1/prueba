B2-ej8

import java.util.Scanner;
/**
 *
 * @author Alumno_2
 */
public class B2_Ej8_Ecuacion {

    /**
     * resolver una ecuacion de primer grado ax+b=0;ax=b;x=-b/a  ---Siempre y cuando a y b no sean 0
     * -¿que pasa cunado a y b son 0?
     * Si a y b son cero sout("Solucion indeterminada");
     * -¿que pasa si a=0?
     * sout("solucion imposible");
     * 
     * Leer a;leer b
     * if (a==0) && (b==0) sout ("solucion indeterminada");
     * else
     * if a==0  sout ("solucion imposible");
     * else 
     * x=-b/a; sout("La solucion es "+x);.
     */
    public static void main(String[] args) {
        Scanner teclado=new Scanner(System.in);
        String cad;
        double a;
        double b;
        double x;
        
        System.out.println("Dime a");
        cad=teclado.nextLine();
        a=Integer.parseInt(cad);
        
        System.out.println("Dime b");
        cad=teclado.nextLine();
        b=Integer.parseInt(cad);
        
        if ((a==0) && (b==0))
                {
                    System.out.println("Solucion indeterminada");
                }
        else
        {
            if (a==0)
            {
                System.out.println("Solucion imposible");
            }
            else
            {
               x=-b/a;
                System.out.println("La solucion es "+x);
            }
        }
        
    }
}

