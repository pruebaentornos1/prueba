B2-ej4

import java.util.Scanner;

public class B2_Ej4_Mayor_igual {

    /**
     * Hacer un programa que lea dos numero por teclado y nos muestre si el primero es
"mayor o igual" que el segundo.
     */
    public static void main(String[] args) {
         Scanner teclado=new Scanner(System.in);
         String cad;
         int n1,n2;
         
         System.out.println("Dime un numero");
         cad=teclado.nextLine();
         n1=Integer.parseInt(cad);
         
         System.out.println("Dime otro numero");
         cad=teclado.nextLine();
         n2=Integer.parseInt(cad);
         
         if (n1>=n2)
         {
             System.out.println("El numero "+n1+" es mayor o igual al numero "+n2);
         }
         
         else
         {
             System.out.println("El numero "+n1+" es menor al numero "+n2);
         }
    }
}

