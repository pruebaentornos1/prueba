B1_Ej4

import java.util.Scanner;

public class B1_Ej4_Celsius_fahrenheit {

    /**
     * Diseña una clase que realice la conversión siguiente: una temperatura dada en
grados Celsius en Fahrenheit.
     */
    public static void main(String[] args) {
        
        Scanner teclado=new Scanner(System.in);
        String cad;
        
        double celsius;
        double far;
        
        System.out.println("Dime los grados Fahrenheit");
        cad=teclado.nextLine();
        far=Integer.parseInt(cad);
        
        celsius= (far-32.0)/1.8;
        
        System.out.println(far+ " grados Fº son "+ celsius+ " grados ºC");
        
        System.out.println("Dime los grados Celsius");
        cad=teclado.nextLine();
        celsius=Integer.parseInt(cad);
        
        far = (9.0*celsius/5.0) +32.0;
        
        System.out.println(celsius+ " grados ºC son "+ far+ " grados ºF");
    }
}

