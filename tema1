Pseudocodigo 15alumnos 

import java.util.Scanner;

public class Pseudocodigo_15alumnos {

    public static void main(String[] args) {
        Scanner tec=new Scanner(System.in);
        String cad;
        int numA,numS;
        double suma,nota,media;
        
        numA=0;
        numS=0;
        suma=0;
        
        for (int contador=1 ;contador<16 ;contador++)
        {
            System.out.println("Dime la nota");
            cad=tec.nextLine();
            nota=Double.parseDouble(cad);
             
            if (nota<5)
            {
                numS=numS+1;
            }
            else
            {
                numA=numA+1;
            }
            suma=suma+nota;
        }
        System.out.println(numA+" aprobados");
        System.out.println(numS+" suspensos");
        
        media=suma/15;
        System.out.println(media+" nota media");
    }
}

