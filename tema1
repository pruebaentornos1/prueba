import java.util.Scanner;
/**
 *
 * @author Alumno_2
 */
public class B2_Ej1_Mayor {

    /**
     * Hacer un programa que lea un numero por teclado y nos diga si mayor que cero
     */
    public static void main(String[] args) {
        Scanner teclado=new Scanner(System.in);
        String cad;
        double numero;
        
        System.out.println("Dime un numero");
        cad=teclado.nextLine();
        numero=Double.parseDouble(cad);
        
        if (numero<=0)
        { 
            System.out.println("Numero menor que cero");
        }
        else
        {
            System.out.println("Numero mayor que cero");
        }
    }
}

