B1_Ej6

import java.util.Scanner;

public class B1_Ej6_Intercambio {

    /**
     * Diseña una clase donde se introduzcan dos variables de tipo int, y se
intercambien sus valores.
     */
    public static void main(String[] args) {
        
        Scanner teclado=new Scanner(System.in);
        String cad;
        int v1;
        int v2;
        int cambio;
        
        System.out.println("Dime la variable 1");
        cad=teclado.nextLine();
        v1=Integer.parseInt(cad);
        
        System.out.println("Dime la variable 2");
        cad=teclado.nextLine();
        v2=Integer.parseInt(cad);
        
        cambio=v1;
        v1=v2;
        v2=cambio;
        
        System.out.println("El nuevo valor de la variable 1 es "+ v1 +" y el nuevo valor de la variable 2 es "+v2);
    }
}

