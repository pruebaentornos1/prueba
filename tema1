Pseudocodigo 8

import java.util.Scanner;

public class Pseudocodigo_8 {

  
    public static void main(String[] args) {
       Scanner teclado=new Scanner(System.in);
       String cad;
       int anyo;
       
        System.out.println("Dime un año");
        cad=teclado.nextLine();
        anyo=Integer.parseInt(cad);
        
        if (anyo>0)
        {
            if ((anyo%4==0) && (anyo%100==0) || (anyo%400==0) )
                    {
                        System.out.println("El año "+anyo+" es bisiesto");
                    }
            else
        {
            System.out.println("El año "+anyo+" no es bisiesto");
        }
        
        else
    {
        System.out.println("El año introducido es incorrecto, debe ser mayor que 0");
    }
        
    }
}
}

