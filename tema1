import java.util.Scanner;
/**
 *
 * @author xps
 */
public class Pseudocodigo_2_EurosDOlares {

    /**
     * Cambiar de euros a dolares
     */
    public static void main(String[] args) {
        Scanner teclado=new Scanner(System.in);
        String cad;
        double euros,dolares,equivalencia;
        
        System.out.println("¿Cuantos euros quieres cambiar?");
        cad=teclado.nextLine();
        euros=Double.parseDouble(cad);
        
        System.out.println("¿Cual es la cotizacion del € $ ?");
        cad=teclado.nextLine();
        equivalencia=Double.parseDouble(cad);
        
        dolares=equivalencia*euros;
        
        System.out.println("Los "+euros+" euros corresponden a "+dolares+" dolares");
    }
}

