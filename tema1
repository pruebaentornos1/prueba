B3-ej11

import java.util.Scanner;

public class B3_Ej11 {

    /**
     * Muestra los N primeros múltiplos de 4, donde N es un valor introducido por
teclado.
     */
    public static void main(String[] args) {
        Scanner tec=new Scanner(System.in);
        
        /*
        
        * 
        * DO WHILE
        int n;
        int mult4;
        
        
        System.out.println("Dime el valor de N");
        n=tec.nextInt();
        
        mult4=0;
        int contador=1;
        
        do
        {
            if (contador%4==0)
            {
                System.out.println(contador+" es multiplo de 4");
                mult4++;
            }
            
            contador++;
        }while (mult4<n);
        
        
        
        *WHILE
        int n;
        int mult4;
        
        
        System.out.println("Dime el valor de N");
        n=tec.nextInt();
        
        mult4=0;
        int contador=1;
        
        while(mult4<n)
                {
                    if (contador%4==0)
                    {
                        System.out.println(contador+" es multiplo de 4");
                        mult4++;
                    }
                    contador++;
                }
        
        */
        
        
        //FOR
        int n;
        int mult4;
        
        
        System.out.println("Dime el valor de N");
        n=tec.nextInt();
        
        
        int contador;
        for(mult4=0,contador=1;mult4<n;contador++)
        {
            if (contador%4==0)
            {
                System.out.println(contador+" es multiplo de 4");
                mult4++;
            }
        }
    }
}

