Pseudocodigo nota15alumnos

import java.util.Scanner;

public class Pseudocodigo_nota15alumnos {

    
    public static void main(String[] args) {
       Scanner tec=new Scanner(System.in);
       String cad;
       int aprobados,contador;
       double nota;
       
       aprobados=0;
       contador=1;
       
       while (contador<=15)
       {
           System.out.println("Dime la nota");
           cad=tec.nextLine();
           nota=Double.parseDouble(cad);
           
           if (nota>=5)
           {
               aprobados=aprobados+1;
           }
           contador=contador+1;
       }
       
        System.out.println("Hay "+aprobados+" alumnos aprobados");
        System.out.println("Hay "+(15-aprobados)+" alumnos suspensos");
        System.out.println("Porcentaje de aprobados es "+(aprobados*0.15));
        System.out.println("Porcentaje de suspensos es "+(15-aprobados)*0.15);
    }
}

