Pseudocodigo Terramitica

import java.util.Scanner;

public class Pseudocodigo_Terramitica {

    
    public static void main(String[] args) {
        Scanner tec=new Scanner(System.in);
        String cad;
        int edad,totalperson,num4,num4_12,numadul;
        double caja;
        
        totalperson=0;
        num4=0;
        num4_12=0;
        numadul=0;
        caja=0;
        
        System.out.println("Dime la edad");
        cad=tec.nextLine();
        edad=Integer.parseInt(cad);
        
        while (edad!=-1)
        {
            if (edad<4)
            {
                num4=num4+1;
            }
            else
              if (edad<=12)
              {
                 num4_12=num4_12+1;
              }
            else
              {
                  numadul=numadul+1;
              }
            
            totalperson=totalperson+1;
            
            System.out.println("Dime la edad");
            cad=tec.nextLine();
            edad=Integer.parseInt(cad);
        }
        
        System.out.println(num4+" menores de 4 años");
        System.out.println(num4_12+" entre 4 y 12 años");
        System.out.println(numadul+" adultos");
        
        caja=num4_12*20+numadul*40;
        System.out.println(caja+" euros recaudados");
    }
}

