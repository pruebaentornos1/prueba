public class B3_Ej7 {

    /* Del 100 al 150 - mostrar multiplo de 3, cuantots multiplos de 5, suma de los pares.
    */
    public static void main(String[] args) {
         int numMult5=0;
         int sumapares=0;
         int contador;
         
         
        /* for (contador=100;contador<=150;contador++)
         {
             if (contador%3==0)
             {
                 System.out.println(contador+" es multiplo de 3");
             }
             if (contador%5==0)
             {
                 System.out.println(contador+" es multiplo de 5");
             }
             if (contador%2==0)
             {
                 sumapares+=contador;
             }
         }
             System.out.println("Entre 100 y 150 hay: "+numMult5+" multiplos de 5 ");
             System.out.println("La suma de los numeros pares entre 100 y 150 es "+sumapares);
         */
         
         contador=100;
         
         while (contador<=150)
         {
             if (contador%3==0)
             {
                 System.out.println(contador);
             }
             if (contador%5==0)
             {
                 System.out.println(contador+" es multiplo de 5");
             }
             if (contador%2==0)
             {
                 sumapares+=contador;
             }
             contador++;
         }
         System.out.println("Entre 100 y 150 hay: "+numMult5+" multiplos de 5 ");
             System.out.println("La suma de los numeros pares entre 100 y 150 es "+sumapares);
         
    }
}

